<h1>Create User</h1>
<%= form_with model: @user, local: true do |form| %> 
  <div>
    <%= form.text_field :first_name, placeholder: "Peter" %>
  </div>
  <div>
    <%= form.text_field :last_name, placeholder: "Griffin" %>
  </div>
  <div>
    <%= form.text_field :email, placeholder: "PeterGriffin@gmail.com" %>
  </div>
  <div>
    <%= form.submit "Save" %>
  </div>
<% end %>
